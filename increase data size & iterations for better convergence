        "# Importing Libraries\n",
        "import numpy as np\n",
        "\n",
        "# Data y = 2x + 50\n",
        "x = np.arange(100)\t\t# 200 causes an overflow in training\n",
        "y = 2 * x + 50\n",
        "\n",
        "# Print first 10 values\n",
        "print(\"x y\")\n",
        "for i in range(10):\n",
        "\tprint(x[i] , y[i])"
      ],
